{
    "name": "dougsisk/laravel-country-state",
    "description": "Country & state helper for Laravel 5.1+.",
    "keywords": ["countries","country","laravel","state","states"],
    "homepage": "https://github.com/DougSisk/laravel-country-state",
    "license": "MIT",
    "authors": [
        {
            "name": "Doug Sisk",
            "homepage": "http://dougsisk.com"
        }
    ],
    "support": {
        "issues": "https://github.com/DougSisk/laravel-country-state/issues"
    },
    "require": {
        "php": ">=7.0",
        "illuminate/support": "~5.1",
        "rinvex/country": "3.1.*"
    },
    "require-dev": {
        "orchestra/testbench": "~3.0",
        "phpunit/phpunit": "~5.0"
    },
    "autoload": {
        "psr-4": {
            "DougSisk\\CountryState\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "DougSisk\\CountryState\\CountryStateServiceProvider"
            ]
        }
    }
}
